{
  "name": "backend-image-feed",
  "version": "1.0.0",
  "description": "NodeJs Rest API using TDD, Clean Architecture and Typescript",
  "main": "index.js",
  "scripts": {
    "start": "node dist/main/server.js",
    "dev": "sucrase-node src/main/server.ts",
    "build": "rimraf dist && tsc -p tsconfig-build.json",
    "up": "npm run build && docker-compose up -d",
    "down": "docker-compose down",
    "test": "jest --passWithNoTests --silent --noStackTrace --runInBand",
    "test:verbose": "jest --passWithNoTests --runInBand",
    "test:unit": "npm test -- --watch -c jest-unit-config.js",
    "test:integration": "npm test -- --watch -c jest-integration-config.js",
    "test:staged": "npm test -- --findRelatedTests",
    "test:ci": "npm test -- --coverage",
    "test:coveralls": "npm run test:ci && coveralls < coverage/lcov.info",
    "pre-commit": "lint-staged",
    "heroku-postbuild": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vagnerolliver/backend-image-feed.git"
  },
  "keywords": [],
  "author": "VÃ¡gner Silveira de Oliveira",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/vagnerolliver/backend-image-feed/issues"
  },
  "homepage": "https://github.com/vagnerolliver/backend-image-feed#readme",
  "devDependencies": {
    "@shelf/jest-mongodb": "^1.2.4",
    "@types/express": "^4.17.11",
    "@types/jest": "^26.0.22",
    "@types/mongodb": "^3.6.12",
    "@types/node": "^14.14.41",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "coveralls": "^3.1.0",
    "eslint": "^7.25.0",
    "eslint-config-standard-with-typescript": "^20.0.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.3.1",
    "eslint-plugin-standard": "^5.0.0",
    "git-commit-msg-linter": "^3.1.0",
    "husky": "^6.0.0",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.4",
    "rimraf": "^3.0.2",
    "sucrase": "^3.18.1",
    "supertest": "^6.1.3",
    "ts-jest": "^26.5.5",
    "typescript": "^4.2.4"
  },
  "dependencies": {
    "express": "^4.17.1",
    "module-alias": "^2.2.2",
    "mongodb": "^3.6.6"
  },
  "engines": {
    "node": "12.x"
  },
  "_moduleAliases": {
    "@": "dist"
  }
}
